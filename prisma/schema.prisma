generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @map("_id") @default(auto()) @db.ObjectId
  username String   @unique
  password String
  age      Int
}
